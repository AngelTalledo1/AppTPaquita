<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppTransporte.Interfaces.VTmisViajes"
             Title="VTmisViajes"
             NavigationPage.HasNavigationBar="False">
    <Grid RowDefinitions=".07*,.9*"
          BackgroundColor="#cb4335">
        <Grid ColumnDefinitions=".1*,.9*">
            <Button BackgroundColor="Transparent"
                    Clicked="Btn_atrasMisViajes" />
            <Image Source="atras.png"
                   HorizontalOptions="Center"
                   HeightRequest="20"
                   WidthRequest="20"
                   Margin="20,20,0,0" />
            <Label Text="Mis Viajes"
                   FontSize="20"
                   FontFamily="Comf-Bold"
                   Grid.Column="1"
                   FontAttributes="Bold"
                   TextColor="White"
                   HorizontalOptions="Start"
                   VerticalOptions="Center"
                   Margin="30,20,0,0" />
        </Grid>
        <Grid Grid.Row="1"
              RowDefinitions=".1*">
            <RoundRectangle CornerRadius="0,0,0,0"
                            Fill="#FBF6FD" />
        </Grid>
    </Grid>
</ContentPage>